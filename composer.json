{
    "name": "QDash PortaSwitch Monitoring Suite",
    "description": "QDash PortaSwitch Monitoring Suite",
    "keywords": ["qdash","PortaSwitch","signalq"],
    "license": "Propietory",
    "authors": [
        {
            "name": "signalQ Solutions",
            "homepage": "http://www.signalq.ca"
        }
    ],
	"repositories": [
	        {
	            "type": "vcs",
	            "url": "https://github.com/Zizaco/ardent.git"
	        }
	],
	"require": {
		"laravel/framework": "4.1.*",
		"laravelbook/ardent": "dev-has-session-fix",
		"zizaco/confide": "dev-master",
		"zizaco/entrust": "dev-master",
		"robclancy/presenter": "1.2.*",
        "j20/php-uuid": "dev-master",
        "bllim/datatables": "*",
		"stolz/assets" : "dev-master",
		"anahkiasen/rocketeer" : "dev-master"
	},
	"require-dev": {
        "way/generators": "dev-master",
        "mockery/mockery": "dev-master@dev",
        "summerstreet/woodling": "0.1.*",
        "lmammino/jsmin4assetic": "1.0.*"
	},
	"autoload": {
		"classmap": [

			"app/commands",
			"app/controllers",
            "app/library",
			"app/models",
			"app/presenters",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/library"
		]
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "source"
	},
	"minimum-stability": "dev"
}
